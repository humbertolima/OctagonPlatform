@model OctagonPlatform.Models.Partner

<table class="table table-striped table-hover " >
    <tbody>
    <tr class="info">

        <th>Business Name: </th><th>@Model.BusinessName</th>
    </tr>
    <tr class="info">

        <th>Parent:</th>
        <th>@Model.Parent.BusinessName</th>
    </tr>
    <tr class="info">

        <th>Free Surcharge Fee Contract Conclude:</th>
        <th>@Model.Parent.FreeSurchargeFeeContractConclude.ToShortDateString()</th>
    </tr>
    <tr class="info">

        <th>Address1:</th>
        <th>@Model.Address1</th>
    </tr>
    <tr class="info">

       <th>Address2:</th> 
        <th>@Model.Address2</th>
    </tr>
    <tr class="info">

        <th>Country:</th>
        <th>@Model.Country.Name</th>
    </tr>
    <tr class="info">

        <th>State:</th>
        <th>@Model.State.Name</th>
    </tr>
    @if (Model.City != null)
    {
        <tr class="info">

            <th>City:</th> <th>@Model.City.Name</th>
        </tr>
    }
    <tr class="info">

        <th>Created At:</th>
        <th>@Model.CreatedAt</th>
    </tr>
    <tr class="info">

        <th>Created By:</th>
        <th>@Model.CreatedByName</th>
    </tr>
    <tr class="info">

        <th>Status:</th>
        <th>@Model.Status</th>
    </tr>
    <tr class="info">

        <th>Email:</th>
        <th>@Model.Email</th>
    </tr>
    <tr class="info">

        <th>Mobil:</th>
        <th>@Model.Mobile</th>
    </tr>
    </tbody>
</table>
    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
        <ul class="nav navbar-nav">
            <li>
                @Html.ActionLink(
                               linkText: "Edit",
                               actionName: "Edit",
                               controllerName: "Partners",
                               routeValues: new
                               {
                                   Id = Model.Id
                               },
                               htmlAttributes: new { @class = "btn btn-primary" }
                               )
            </li>


        </ul>

    </div>
