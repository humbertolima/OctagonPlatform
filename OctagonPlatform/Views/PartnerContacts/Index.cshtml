@model IEnumerable<OctagonPlatform.Models.PartnerContact>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdminSB.cshtml";
}

<div class="page-header">
    <h3 class="panel-title">List Partners</h3>
</div>
<p>
    @Html.ActionLink("Add PartnerContact", "Create", "PartnerContacts", routeValues:null, htmlAttributes: new{ @class = "btn btn-info" })
</p>
<h3>@ViewBag.Error</h3>
<table class="table table-striped table-hover ">
    <thead>
    <tr>
        <th>Partner</th>
        <th>Name</th>
        <th>Last Name</th>
        <th>Phone</th>
        <th>Email</th>
        <th>Addres</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr class="info">
            <td>@item.Partner.BusinessName</td>
            <td>@item.Name</td>
            <td>@item.LastName</td>
            <td>@item.Phone</td>
            <td>@item.Email</td>
            <td>@item.Address1</td>
            <td>@Html.ActionLink("Edit", "Edit", new {id = item.Id})</td>
            <td>@Html.ActionLink("Delete", "Delete", new { id = item.Id })</td>
            <td>@Html.ActionLink("Details", "", new { id = item.Id })</td>
        </tr>
    }
    
</table> 
        


