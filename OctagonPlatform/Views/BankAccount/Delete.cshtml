@model OctagonPlatform.Models.FormsViewModels.BAEditFVModel
@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_LayoutAdminSB.cshtml";
}


<h3 class="alert-danger">Delete BankAccount</h3>

<h3 class="alert-warning">Are you sure you want to delete this?</h3>
<div>
    <dl class="dl-horizontal">
        <dt>
            Partner
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Partner.BusinessName)
        </dd>


        <dt>
            NickName
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NickName)
        </dd>

        <dt>
            Status
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Status)
        </dd>
        <dt>
            Routing Number
        </dt>

        <dd>
            @Html.DisplayFor(model => model.RoutingNumber)
        </dd>
        <dt>
            Account Number
        </dt>

        <dd>
            @Html.DisplayFor(model => model.AccountNumber)
        </dd>

        <dt>
            Email
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Email)
        </dd>

        <dt>
            Phone
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Phone)
        </dd>

        <dt>
            Name on Check
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NameOnCheck)
        </dd>
        <dt>
            Account Type
        </dt>

        <dd>
            @Html.DisplayFor(model => model.AccountType)
        </dd>
    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-group">
            <input type="submit" value="Delete" class="btn btn-primary" /> |
            <button type="reset" onclick="history.back();" class="btn btn-default">Cancel</button>
        </div>
    }
</div>


