@model OctagonPlatform.Models.FormsViewModels.UserEditFormViewModel
@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_LayoutAdminSB.cshtml";
}


<h3  class="alert-danger">Delete</h3>

<h3 class="alert-warning">Are you sure you want to delete this?</h3>
<div>
    <dl class="dl-horizontal">
        <dt>
            Partner
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Partner.BusinessName)
        </dd>


        <dt>
            UserName
        </dt>

        <dd>
            @Html.DisplayFor(model => model.UserName)
        </dd>

        <dt>
            Status
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Status)
        </dd>
        <dt>
            Name
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt>
            Last Name
        </dt>

        <dd>
            @Html.DisplayFor(model => model.LastName)
        </dd>

        <dt>
            Email
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Email)
        </dd>

        <dt>
           Phone
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Phone)
        </dd>

        <dt>
            IsLocked
        </dt>

        <dd>
            @Html.DisplayFor(model => model.IsLocked)
        </dd>
    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-group">
            <input type="submit" value="Delete" class="btn btn-primary" /> |
            <button type="reset" onclick="history.back();" class="btn btn-default">Cancel</button>
        </div>
    }
</div>


