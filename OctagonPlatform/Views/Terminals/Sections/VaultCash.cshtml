@model OctagonPlatform.Models.VaultCash


<div id="getVaultCash" class="panel-collapse collapse">
    @Ajax.ActionLink("ADD", "Create", "VaultCash", new { terminalId = ViewData["terminalId"]?.ToString() },
                   new AjaxOptions
                   {
                       UpdateTargetId = "AddVaultCash",
                       LoadingElementId = "loading3",
                       InsertionMode = InsertionMode.Replace,
                       HttpMethod = "Get"
                   },
                   htmlAttributes: new
                   {
                       @class = "btn btn-info",
                       data_toggle = "modal",
                       data_target = "#AddVaultCash"
                   })
    <table id="vaultcash" class="table table-striped table-hover table-bordered">
        <thead class="thead-blue">
            <tr>
                <th>Bank Acc</th>
                <th>Bank Name</th>
                <th>Effective Date</th>
                <th>Stop Date</th>
            </tr>
        </thead>
        @if (Model != null)
        {
            <tbody>
                <tr>
                    <td>@Model.BankAccount.NickName</td>
                    <td>@Model.BankAccount.BankName</td>
                    <td>@Model.StartDate.ToShortDateString()</td>
                    <td>@Model.StopDate.ToShortDateString()</td>
                </tr>
            </tbody>
        }
    </table>
</div>