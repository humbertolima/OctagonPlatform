<!DOCTYPE html>
<title>Octagon</title>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title </title>
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Raleway:400,700">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Raleway">
    @Styles.Render("~/Octagon/assets/css/styles.css")
    @Styles.Render("~/Octagon/assets/css/Contact-Form-Clean.css")
    @Styles.Render("~/Octagon/assets/css/bootstrap.min.css")
    @Styles.Render("~/Octagon/assets/css/Contact-FormModal-Contact-Form-with-Google-Map.css")
    @Styles.Render("~/Octagon/assets/css/Footer-Basic.css")
    @Styles.Render("~/Octagon/assets/css/Map-Clean.css")
    @Styles.Render("~/Octagon/assets/css/Navigation-with-Button1.css")
    @Styles.Render("~/Octagon/assets/css/styles-1.css")
    @Styles.Render("~/Octagon/assets/css/styles.css")
    @Styles.Render("~/Octagon/assets/css/styles2.css")
    @Styles.Render("~/Octagon/assets/css/untitled-1.css")
    @Styles.Render("~/Octagon/assets/css/untitled.css")

    @Styles.Render("~/Octagon/assets/fonts/font-awesome.min.css")
    @Styles.Render("~/Octagon/assets/fonts/simple-line-icons.min.css")

    @Styles.Render("~/Octagon/assets/bootstrap/css/bootstrap.min.css")

    <link rel="icon" type="image/png" href="~/Octagon/assets/img/favicon-32x32.png" sizes="32x32" />
    <link rel="icon" type="image/png" href="~/Octagon/assets/img/favicon-16x16.png" sizes="16x16" />
</head>
<body>
@RenderBody()
@Scripts.Render("~/Octagon/assets/js/jquery.min.js")
@Scripts.Render("~/Octagon/bootstrap/js/bootstrap.min.js")
@Scripts.Render("~/Octagon/assets/js/Contact-FormModal-Contact-Form-with-Google-Map.js")
@RenderSection("scripts", required: false)
</body>
@Html.Partial("Sections/Footer")
</html>
